{
  "name": "@inkwell/auth-service",
  "version": "1.0.0",
  "description": "Authentication service for InkWell AI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "NODE_ENV=test dotenv -e .env.test -- jest --config=jest.config.js",
    "test:watch": "NODE_ENV=test dotenv -e .env.test -- jest --watch --config=jest.config.js",
    "test:coverage": "NODE_ENV=test dotenv -e .env.test -- jest --coverage --config=jest.config.js",
    "test:debug": "NODE_ENV=test dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config=jest.config.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "db:setup": "tsx test/setup-db.ts",
    "db:test": "tsx test/pg-connection-test.ts"
  },
  "dependencies": {
    "@fastify/cors": "^8.0.0",
    "@fastify/helmet": "^11.0.0",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.11",
    "@prisma/client": "^6.16.1",
    "@types/pg": "^8.15.5",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "fastify": "^4.0.0",
    "fastify-plugin": "^4.5.0",
    "google-protobuf": "^3.21.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.16.3",
    "ts-proto": "^1.169.0",
    "tsx": "^3.12.0",
    "typescript": "^5.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@jest/globals": "^30.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.14",
    "@types/pg": "^8.10.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.1.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^3.12.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
